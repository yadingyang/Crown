@model IEnumerable<CM.Models.Product>
@{
     ViewBag.Title = "Cate";
}

<h2>Cate</h2>
<table>
    @foreach (var item in Model)
    {
        <tr>
            <td>
                @Html.ActionLink(item.Name, "Detail", new
        {
            id = item.ProductId
        })
            </td>  
           

        </tr>
    }
</table>